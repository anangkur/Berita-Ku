<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/ConstraintDefault">
    <FrameLayout
        android:id="@+id/frame_main"
        style="@style/FrameDefault"
        android:layout_height="@dimen/dimens_0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@id/bottom_nav_main"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"/>
    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottom_nav_main"
        style="@style/BottomNavigationDefault"
        app:menu="@menu/menu_bottom_nav"
        app:itemIconTint="@drawable/color_bottom_nav"
        app:itemTextColor="@drawable/color_bottom_nav"
        app:layout_constraintTop_toBottomOf="@id/frame_main"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>